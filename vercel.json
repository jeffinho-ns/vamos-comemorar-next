{
  "buildCommand": "yarn build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "yarn install",
  "devCommand": "yarn dev",
  "regions": ["gru1"],
  "functions": {
    "app/cardapio/[slug]/page.tsx": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "SHARP_IGNORE_GLOBAL_LIBVIPS": "1"
    }
  }
}
